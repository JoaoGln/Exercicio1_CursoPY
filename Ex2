primeiro_valor = input('Digite um valor: ')
segundo_valor = input('Digite outro valor: ')

float_primeiro_valor = float(primeiro_valor)
float_segundo_valor = float(segundo_valor)

if float_primeiro_valor > float_segundo_valor:
    print(f'O {primeiro_valor=} é maior que o {segundo_valor=}')

elif float_segundo_valor > float_primeiro_valor:
    print(f'O {segundo_valor=} é maior que o {primeiro_valor=}')
